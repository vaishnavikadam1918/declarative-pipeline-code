pipeline {
    agent any

    stages {
        stage('pull') {
            steps {
                git branch: 'master', url: https://github.com/iamtruptimane/student-app.git
            }
        }
        stage('build') {
            steps {
                echo 'build-job'
            }
        }
        stage('test') {
            steps {
                echo 'test-job'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy-job'
            }
        }
    }
}
